<h1>Listing mc_exporters</h1>

<table>
  <tr>
    <th>Last run</th>
    <th>Puppet classes</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mc_exporters.each do |mc_exporter| %>
  <tr>
    <td><%= mc_exporter.last_run %></td>
    <td><%= mc_exporter.puppet_classes %></td>
    <td><%= link_to 'Show', mc_exporter %></td>
    <td><%= link_to 'Edit', edit_mc_exporter_path(mc_exporter) %></td>
    <td><%= link_to 'Destroy', mc_exporter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

